@model IEnumerable<LibraryMVC.Models.Library>

@{
    ViewBag.Title = "Raamatud";
}

<h2>Raamatud</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Author)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rent_D)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rent_L)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Avail)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Author)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rent_D)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rent_L)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Avail)
        </td>
    </tr>
}

</table>
